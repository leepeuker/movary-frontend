{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "plugin:vue/vue3-recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["vue"],
    "rules": {
        "eslint-plugin-import/no-extraneous-dependencies": 0,
        "import/no-extraneous-dependencies": 0,
        "vue/multi-word-component-names": 0
    }
}
